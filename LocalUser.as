package{		import flash.media.*;	public class LocalUser{				public var camera:Camera = new Camera();		public var microphone:Microphone = new Microphone();				public function LocalUser(){			// get the users camera and microphone			camera = Camera.getCamera();			microphone = Microphone.getMicrophone();					// here are all the quality and performance settings that we suggest			camera.setMode(238, 132, 12, false);			camera.setQuality(0, 75);			camera.setKeyFrameInterval(24);			microphone.rate = 11;			microphone.setSilenceLevel(0);		}	}	}