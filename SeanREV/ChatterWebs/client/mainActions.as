var netC:NetConnection;var netPut:NetStream;var netGet:NetStream;var netGet2:NetStream;var camera:Camera;var mic:Microphone;var video:Video;startChat_btn.addEventListener(MouseEvent.CLICK, doStart);netC = new NetConnection();netC.connect("rtmp://207.71.215.101/chatterWebs");	netC.addEventListener(NetStatusEvent.NET_STATUS, netCOnStatus);function doStart(event:MouseEvent):void {	startChat_btn.removeEventListener(MouseEvent.CLICK, doStart);	stopChat_btn.addEventListener(MouseEvent.CLICK, doStop);		var bandwidth:uint = 0;	var quality:uint = 50;	camera = Camera.getCamera();	mic = Microphone.getMicrophone();		camera.setQuality(bandwidth,quality);	camera.setMode(320,240,30,true);		video = new Video();	video.attachCamera(camera);	addChild(video);	video.x = 50;	video.y = 50;		startTransmit();	}function doStop(event:MouseEvent):void {			getStream1.attachCamera(null);		netGet.play(null);		netGet.close();		getStream1.clear();				getStream2.attachCamera(null);		netGet2.play(null);		netGet2.close();		getStream2.clear();				stopChat_btn.removeEventListener(MouseEvent.CLICK, doStop);		startChat_btn.addEventListener(MouseEvent.CLICK, doStart);				netPut.publish("null");}var getStream1:Video;var getStream2:Video;function startTransmit():void {		netPut = new NetStream(netC);		netPut.publish(inputName_txt.text);	netPut.attachCamera(camera);	netPut.attachAudio(mic);	netPut.bufferTime = 0;		netGet = new NetStream(netC);	netGet.play("sean");	netGet.bufferTime = 0;		netGet2 = new NetStream(netC);	netGet2.play("eric");	netGet2.bufferTime = 0;		getStream1 = new Video();	getStream1.attachNetStream(netGet);	addChild(getStream1);	getStream1.x = 400;	getStream1.y = 50;		getStream2 = new Video();	getStream2.attachNetStream(netGet2);	addChild(getStream2);	getStream2.x = 750;	getStream2.y = 50;	}function netCOnStatus(infoObject:NetStatusEvent){	trace("netC: "+infoObject.info.code+" ("+infoObject.info.description+")");	if (infoObject.info.code == "NetConnection.Connect.Failed")		trace("Connection failed: Try rtmp://[server-ip-address]/s3");	else if (infoObject.info.code == "NetConnection.Connect.Rejected")		trace(infoObject.info.description);}